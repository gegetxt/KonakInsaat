										<!-- Form elements -->
										<div class="row">
											<div class="col-md-12 form-group">
												<input name="adi" type="text" maxlength="100" placeholder="Name Surname *" required>
											</div>
											<div class="col-md-12 form-group">
												<input name="mail" type="email" maxlength="100" placeholder=" E-Mail  *" required>
											</div>
											<div class="col-md-12 form-group">
												<input name="tel" type="text" maxlength="100" placeholder="Phone ">
											</div>
											<div class="col-md-12">
												<button type="submit" name="mgonder" class="btn btn-modern" style="color:black;" onClick="window.location.reload();">SEND MESSAGE</button>

											</div>
										</div>
									</form>
								</div>
							</div>
